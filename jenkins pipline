pipeline {
    agent any

    stages {
        stage('Cloning') {
            steps {
                git 'https://github.com/BSaiSameer/task1'
            }
        }

        stage('Deploying') {
            steps {
                    sh 'rsync -a /var/lib/jenkins/workspace/task1 root@172.31.7.83:/root/'
            }
        }
    }
}
